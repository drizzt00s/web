<link rel='stylesheet' href='/stylesheets/home.css' />
<div class='main' ng-controller='userImage'>
	<div class='headerWrap'>
		<img src='/images/home/index_logo.png' class='logo' />
		<breadcrumb></breadcrumb>
		<div class='userstatusWrap' ng-if='isLogin'>
			<userstatus></userstatus>
		</div>
	</div>

	<div class='main_left'>
		<div class='main_leftWrap'>
			<div class='cpPreview login'>
				<div ng-if='isLogin'>
					<cppreview></cppreview>
				</div>
				<div ng-if='!isLogin'>
					<register></register>
				</div>
			</div>

			<div class='toolList'>
				<toolist></toolist>
			</div>
		</div>

	</div>

	<div class='main_right'>
		<div>
			<img class='img' src='{{img}}' ng-repeat='img in imgList' ng-click='selectImg($event);'/>
		</div>

		<button ng-click='setProfile()'>设为头像</button>


	</div>
</div>