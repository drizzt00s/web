<script src="/javascript/login/login.js"></script>
<link rel='stylesheet' href='/stylesheets/login.css' />

<div ng-controller='login' class="main">
	<div class='loginPanel'>
		<div class='username'>
			<label>登录账号:</label><input type='text' ng-model='username' />
		</div>
		<div class='password'>
			<label>密码:</label><input type='password' ng-model='pass'/>
		</div>
		<button ng-click='login()'>登录</button>
	</div>
</div>

