<script src="/javascript/landing/landing.js"></script>
<script src="/javascript/directive/registerAndLogin.js"></script>
<script src="/javascript/tools/jquery.slideBox.js" type="text/javascript"></script>

<link rel='stylesheet' href='/stylesheets/directive/directive_cppreview.css' />
<link rel='stylesheet' href='/stylesheets/directive/directive_main_side_menu.css' />
<link rel='stylesheet' href='/stylesheets/landing/landing.css' />
<link rel="stylesheet" href="/stylesheets/tools/jquery.slideBox.css"  type="text/css" />

<div class="main alignedCenter">
	<div class="header">
		<div class="landingScroll float-left" style="border:solid 1px red;">
			<div id="slideBox" class="slideBox">
				<ul class="items">
					<li><a href="#" title=""><img src="/images/default/tools/landing_slide/landing_slide1.jpg"></a></li>
					<li><a href="#" title=""><img src="/images/default/tools/landing_slide/landing_slide2.jpg"></a></li>
				</ul>
			</div>
		</div>

		<div class="login float-right">
			<register></register>
		</div>
	</div>

	<div id="showingPic" ng-controller='landing'>
		<div class="col-sm-6 col-md-3" ng-repeat = "user in allUsers">
			<div class="thumbnail">
				<a href="/user/userDetails.ejs?check={{::user.account}}"><img src="{{::user.profile}}"></a>
				<div class="caption text-center">
					<h5>{{::user.falseName}}</h5>
					<p>{{::user.age}}</p>
					<p>{{::user.address}}</p>
					<p><a href="/user/userDetails.ejs?check={{::user.account}}" class="btn btn-primary" role="button"><span class="glyphicon glyphicon-heart"></span>去看看</a></p>
				</div>
			</div>
		</div>
	</div>
</div>



