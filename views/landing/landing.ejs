<script src="/javascript/landing/landing.js"></script>
<script src="/javascript/tools/jquery.slideBox.js" type="text/javascript"></script>

<link rel='stylesheet' href='/stylesheets/directive/directive_main_side_menu.css' />
<link rel='stylesheet' href='/stylesheets/landing/landing.css' />
<link rel="stylesheet" href="/stylesheets/tools/jquery.slideBox.css"  type="text/css" />



<div class="landMain" ng-controller='landing'>
	<div class='landMain_main'></div>

	<div class='regAndLoginWrap'>
		<div class='regAndLogin'>

		</div>
	</div>

	<div class='landMain_quickLogin'>
		<div class='quickLogin_acc username'><label>登录账号:</label><input type='text' class='quickLogin_accInput' ng-model='username' /><div class='error'></div></div>
		<div class='quickLogin_ps password'><label>登录密码:</label><input type='password' class='quickLogin_psInput' ng-model='pass' /><div class='error'></div></div>
		<div class='quickLogin_loginButton'>
			<button class='quickLogin' ng-click='login()'>立即登录 遇见有缘人</button><br />
			<div class='rememberPass'>
				<input type='checkbox' class='rememberPassCheckbox'/><label style='margin-left:1px;'>记住密码(公共场所慎用)</label>
				<a href='#' class='forgetPassLink'>忘记密码?</a>
			</div>
		</div>
	</div>

	<div class='avatarWall'>
	</div>

	<div class='landing_gap'>

	</div>

	<div class='falseAvatar row'>

			<div class="col-md-4" ng-repeat = "user in allUsers">
			<div class="thumbnail">
				<a href="#" class='thumbnailImg'><img src="{{::user.profile}}"></a>
				<div class="caption text-center">
					<h5>{{::user.falseName}}</h5>
					<p>{{::user.age}}</p>
					<p>{{::user.address}}</p>
					<p><span class="landing_check" role="button">去看看</span></p>
				</div>
			</div>
		</div>

	</div>

</div>



